{
  "name": "slack-notification",
  "version": "1.0.0",
  "description": "notification sender for my private slack ;)",
  "main": "src/app.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p .",
    "build:watch": "tsc -w -p .",
    "format": "biome format --write src/",
    "lint": "biome lint src/",
    "check": "biome check --apply src/",
    "start": "node dist/app.js",
    "prepare": "husky"
  },
  "repository": "git+https://github.com/hikariru/slack-notification.git",
  "author": " ",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hikariru/slack-notification/issues"
  },
  "homepage": "https://github.com/hikariru/slack-notification#readme",
  "engines": {
    "node": "24.x"
  },
  "dependencies": {
    "@slack/bolt": "^4.4.0",
    "@types/express": "^5.0.3",
    "@types/express-serve-static-core": "^5.0.7",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.1.0",
    "dotenv": "^17.2.1",
    "luxon": "^3.7.1",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2"
  },
  "volta": {
    "node": "24.5.0"
  }
